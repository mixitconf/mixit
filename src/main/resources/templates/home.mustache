<div class="row mxt-layout-header-effect mxt-section"><div class="mxt-layout-header-effect-grey-inversed"></div></div>
<div class="mxt-layout-main-accent-section">
    <div class="container-xxl ">
        <h2>{{#i18n}}home.about{{/i18n}}</h2>

        <div class="row pt-2">
            <div class="col-md-6">
                <div class="mxt-layout-main-outline">{{#i18n}}home.about.outline{{/i18n}}</div>
                <br/>
                <p class="lead">{{#i18n}}home.about.description{{/i18n}}</p>
                <p>{{#i18n}}home.about.wishes{{/i18n}}</p>

                <div class="row">
                    <div class="mxt-header--accesibility-container">
                        <img src="/images/svg/accessibility/mxt-icon--child-care-dark.svg"
                             class="mxt-icon--social"
                             alt="{{#i18n}}accessibility.childcare{{/i18n}}">
                        <div>{{#i18n}}accessibility.text.mummy{{/i18n}}</div>
                    </div>
                    <div class="mxt-header--accesibility-container">
                        <img src="/images/svg/accessibility/mxt-icon--hearing-impaired-dark.svg"
                             class="mxt-icon--social"
                             alt="{{#i18n}}accessibility.hearing.impaired{{/i18n}}">
                        <div>{{#i18n}}accessibility.hearing.impaired{{/i18n}}</div>
                    </div>
                    <div class="mxt-header--accesibility-container">
                        <img src="/images/svg/accessibility/mxt-icon--motor-disability-dark.svg"
                             class="mxt-icon--social"
                             alt="{{#i18n}}accessibility.motor.disability{{/i18n}}">
                        <div>{{#i18n}}accessibility.motor.disability{{/i18n}}</div>
                    </div>
                    <div class="mxt-header--accesibility-container">
                        <img src="/images/svg/accessibility/mxt-icon--pregnant-dark.svg"
                             class="mxt-icon--social"
                             alt="{{#i18n}}accessibility.pregnant{{/i18n}}">
                        <div>{{#i18n}}accessibility.pregnant{{/i18n}}</div>
                    </div>
                </div>
                <p>
                    {{#i18n}}accessibility.text.moblity{{/i18n}}
                    <a href="/accessibility"><strong>{{#i18n}}accessibility.plus{{/i18n}}</strong></a>
                </p>
            </div>
            <div class="col-md-6">
                <div class="mxt-wrapper--intrinsic">
                    <iframe src="https://player.vimeo.com/video/840545012" class="mxt-wrapper--video-player vimeo"
                            allowfullscreen></iframe>
                </div>
                <div class="mxt-header--stats">
                    <div>{{#i18n}}home.stat1{{/i18n}}</div>
                    <div>{{#i18n}}home.stat2{{/i18n}}</div>
                    <div>{{#i18n}}home.stat3{{/i18n}}</div>
                    <div>{{#i18n}}home.stat4{{/i18n}}</div>
                    <div>{{#i18n}}home.stat5{{/i18n}}</div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row mxt-layout-header-effect"><div class="mxt-layout-header-effect-grey"></div></div>

<div class="container-xxl mxt-section">
    <h2>{{#i18n}}home.talk.intro{{/i18n}}</h2>
    <p>{{#i18n}}home.talk.description{{/i18n}}</p>
    <div class="mxt-tracks row">
        <div class="col-12 col-md-6 col-lg-3 row pt-2">
            <div class="col-4 col-md-3 col-lg-3 text-center">
                <img src="/images/svg/mxt-icon--tech.svg" class="mxt-icon--cat__home"
                     alt="{{#i18n}}topics.tech.title{{/i18n}}">
            </div>
            <div class="col-8 col-md-9 col-lg-9">
                <h3 style="margin-bottom: -0.2rem;" class="pt-0">{{#i18n}}topics.tech.title{{/i18n}}</h3>
                <small>{{#i18n}}topics.tech.description{{/i18n}}</small>
            </div>
        </div>
        <div class="col-12 col-md-6 col-lg-3 row pt-2">
            <div class="col-4 col-md-3 col-lg-3 text-center">
                <img src="/images/svg/mxt-icon--aliens.svg" class="mxt-icon--cat__home"
                     alt="{{#i18n}}topics.aliens.title{{/i18n}}">
            </div>
            <div class="col-8 col-md-9 col-lg-9">
                <h3 style="margin-bottom: -0.2rem;" class="pt-0">{{#i18n}}topics.aliens.title{{/i18n}}</h3>
                <small>{{#i18n}}topics.aliens.description{{/i18n}}</small>
            </div>
        </div>
        <div class="col-12 col-md-6 col-lg-3 row pt-2">
            <div class="col-4 col-md-3 col-lg-3 text-center">
                <img src="/images/svg/mxt-icon--team.svg" class="mxt-icon--cat__home"
                     alt="{{#i18n}}topics.team.title{{/i18n}}">
            </div>
            <div class="col-8 col-md-9 col-lg-9">
                <h3 style="margin-bottom: -0.2rem;" class="pt-0">{{#i18n}}topics.team.title{{/i18n}}</h3>
                <small>{{#i18n}}topics.team.description{{/i18n}}</small>
            </div>
        </div>
        <div class="col-12 col-md-6 col-lg-3 row pt-2">
            <div class="col-4 col-md-3 col-lg-3 text-center">
                <img src="/images/svg/mxt-icon--ethics.svg" class="mxt-icon--cat__home"
                     alt="{{#i18n}}topics.ethics.title{{/i18n}}">
            </div>
            <div class="col-8 col-md-9 col-lg-9">
                <h3 style="margin-bottom: -0.2rem;" class="pt-0">{{#i18n}}topics.ethics.title{{/i18n}}</h3>
                <small>{{#i18n}}topics.ethics.description{{/i18n}}</small>
            </div>
        </div>
    </div>

    <p class="lead">{{#i18n}}home.talk.alien{{/i18n}}</p>

    <div class="mxt-video--player-container">
        {{#aliens}}
            {{>talk-video-viewer}}
        {{/aliens}}
    </div>
</div>
