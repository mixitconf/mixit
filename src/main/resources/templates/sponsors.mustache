            <div class="mxt-wrapper--callout callout">
                <h2 class="text-center mxt-subheader--space">{{#i18n}}sponsors.party{{/i18n}}</h2>
                {{#sponsors-party}}
                    {{> sponsor }}
                {{/sponsors-party}}
            </div>

            <div class="mxt-wrapper--callout callout">
                <h2 class="text-center mxt-subheader--space">{{#i18n}}sponsors.hosting{{/i18n}}</h2>
                {{#sponsors-hosting}}
